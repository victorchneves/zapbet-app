{
    "crons": [
        {
            "path": "/api/cron/daily_update",
            "schedule": "0 8 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}